<!DOCTYPE html>
<html class="loading" lang="en">
<head>
    <%- include('../../partials/head') %>
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.12.1/css/jquery.dataTables.css">
	 <link rel="stylesheet" type="text/css" href="/assets/css/style.css">
	 <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.css" />
</head>
<body class="vertical-layout vertical-menu 2-columns menu-expanded fixed-navbar" data-open="click" data-menu="vertical-menu" data-color="bg-chartbg" data-col="2-columns">
    <%- include('../../partials/nav_header') %>
    <%- include('../../partials/nav_main') %>

    <div class="app-content content">
        <div class="content-wrapper">
            <div class="content-wrapper-before"></div>
            <div class="content-header row">
                <div class="content-header-left col-md-4 col-12 mb-2">
                    <h3 class="content-header-title">Edit Symptom</h3>
                </div>
                <div class="content-header-right col-md-8 col-12">
                    <div class="breadcrumbs-top float-md-right">
                        <div class="breadcrumb-wrapper mr-1">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="/">Dashboard</a>
                            </li>
                            <li class="breadcrumb-item active"><a href="/symptom">Symptom</a>
                            </li>
                            <li class="breadcrumb-item active">Edit
                            </li>
                        </ol>
                        </div>
                    </div>
                </div>
                <div class="content-body col-12">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="card">
                                <div class="card-header">
                                    <h4 class="card-title">Edit</h4>
                                </div>
                                <div class="card-block">
                                    <div class="card-body">
                                        <fieldset class="form-group">
                                            <fieldset class="form-group">
                                            <h5 class="mt-2">Title *</h5>
                                            <input type="text" class="form-control" id="editTxtTitle">
                                        </fieldset>
                                        <fieldset class="form-group">
                                            <fieldset class="form-group">
                                            <h5 class="mt-2">Description *</h5>
                                            <input type="text" class="form-control" id="editTxtDesc">
                                        </fieldset>
                                        <fieldset class="form-group">
                                            <fieldset class="form-group">
                                            <h5 class="mt-2">Image</h5>
											<img id="imageView" name="imageView" src="" alt="Change Icon" style="background-color: #C21C69;padding: 5px;width: 300px;"><br><br>
                                            <input type="file" id="imageEdit" name="imageEdit" />
                                            <p class="text-muted">Pastikan format photo JPG. <code>(Ukuran 300px x 300px)</code></p>
                                        </fieldset>
                                        <fieldset class="form-group">
                                            <fieldset class="form-group">
                                            <h5 class="mt-2">Publish?</h5>
                                            <select class="custom-select" id="editPublishedSelect" name="editPublishedSelect">
                                                <option value="true">Publish</option>
                                                <option value="false">Draft</option>
                                            </select>
                                        </fieldset>
													 
                                        <div class="alert alert-warning" role="alert" id="alert-warning" style="display: none;"></div>
                                        <fieldset class="form-group">
                                            <a href="/symptom"><button type="button" class="btn btn-secondary btn-min-width mr-1 mb-1">Back</button></a>
                                            <button type="button" onclick="editSubmit()" class="btn btn-primary btn-min-width mr-1 mb-1">Submit</button>
                                        </fieldset>
                                        <div class="alert alert-success" role="alert" id="alert-success" style="display: none;"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <%- include('../../partials/footer') %>
	 
	 
	 <script type="text/javascript" src="https://cdn.jsdelivr.net/momentjs/latest/moment.min.js"></script>
	 <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.min.js"></script>
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.12.1/js/jquery.dataTables.js"></script>
    <script src="/function/global.js" type="text/javascript"></script>
    <script src="/function/symptom-function.js" type="text/javascript"></script>
    <script src="/assets/js/nicEdit.js" type="text/javascript"></script>
	 <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
	 <script src="https://cdn.jsdelivr.net/npm/gasparesganga-jquery-loading-overlay@2.1.7/dist/loadingoverlay.min.js" type="text/javascript"></script>
</body>
</html>