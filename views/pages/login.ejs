<!DOCTYPE html>
<html class="loading" lang="en">
<head>
    <%- include('../partials/head') %>
</head>
<body class="vertical-layout" data-open="click" data-menu="vertical-menu" data-color="bg-chartbg" data-col="4-columns">
    <div class="app-content content">
        <div class="content-wrapper">
            <div class="content-wrapper-before"></div>
            <div class="content-body">
                <selection class="basic-inputs">
                    <div class="row match-height"  style="margin-top: 150px;">
                        <div class="col-xl-12 col-lg-12 col-md-12">
                            <div class="card">
                                <div class="card-header">
                                    <h4 class="card-title">Login Admin MyHeart</h4>
                                </div>
                                <div class="card-block">
                                    <div class="card-body">
                                        <fieldset class="form-group">
                                            <input type="email" class="form-control" id="email" placeholder="Enter Email Address">
                                        </fieldset>
                                        <fieldset class="form-group">
                                            <input type="password" class="form-control" id="password" placeholder="Password">
                                        </fieldset>
                                        <div class="alert alert-warning" role="alert" id="alert" style="display: none;"></div>
                                        <button type="button" id="btn-submit" onClick="submit()" class="btn btn-primary btn-min-width mr-1 mb-1">Login</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </selection>
            </div>
        </div>
    </div>
    <script src="/theme-assets/vendors/js/vendors.min.js" type="text/javascript"></script>
    <script src="/function/global.js" type="text/javascript"></script>
    <script src="/function/auth-function.js" type="text/javascript"></script>
    <script src="https://cdn.jsdelivr.net/npm/gasparesganga-jquery-loading-overlay@2.1.7/dist/loadingoverlay.min.js" type="text/javascript"></script>
</body>
</html>